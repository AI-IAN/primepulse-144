# PrimePulse 144 Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=development
API_KEY=your-api-key-here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/primepulse
DUCKDB_PATH=./data/analytics.duckdb

# Amazon Scraping Configuration
BRIGHTDATA_USERNAME=your-brightdata-username
BRIGHTDATA_PASSWORD=your-brightdata-password
BRIGHTDATA_ENDPOINT=zproxy.lum-superproxy.io:22225
USER_AGENTS_POOL=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Cloudflare Workers Configuration
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
CLOUDFLARE_ZONE_ID=your-cloudflare-zone-id

# Machine Learning Configuration
AWS_LAMBDA_FUNCTION_NAME=primepulse-xgboost-predictor
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# Notification Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SLACK_CHANNEL=#price-alerts
SLACK_USERNAME=PrimePulse Bot

# Google Sheets Integration
GOOGLE_SHEETS_CLIENT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_SHEETS_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY\n-----END PRIVATE KEY-----\n"
GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id

# Data Export Configuration
S3_BUCKET_NAME=primepulse-data-dumps
S3_REGION=us-east-1
BIGQUERY_PROJECT_ID=your-gcp-project-id
BIGQUERY_DATASET_ID=primepulse_analytics

# n8n Workflow Configuration
N8N_WEBHOOK_URL=http://localhost:5678/webhook/primepulse
N8N_API_KEY=your-n8n-api-key

# Monitoring Configuration
GRAFANA_API_URL=http://localhost:3001
GRAFANA_API_KEY=your-grafana-api-key

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Crawling Configuration
CRAWL_INTERVAL_MINUTES=120
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT_MS=30000
RETRY_ATTEMPTS=3
RETRY_DELAY_MS=5000

# Feature Flags
ENABLE_ML_PREDICTIONS=true
ENABLE_SLACK_ALERTS=true
ENABLE_DATA_EXPORTS=true
ENABLE_MONITORING=true

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_PATH=./logs/primepulse.log
